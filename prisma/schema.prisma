generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Investor {
  id             Int      @id @default(autoincrement())
  name           String
  email          String
  twitterPicture String
  company        String
  title          String
  checkSize      String
  details        String
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
